        <action_name>Create Incident</action_name>
        <active>false</active>
        <client>false</client>
        <client_script_v2><![CDATA[function onClick(g_form) {var gr = new GlideRecord('incident');
gr.short_description = current.short_description;
gr.description = current.description;
gr.caller_id = current.u_reference_1;
gr.u_cost_center = current.u_cost_center;
gr.category = 'apps';
gr.contact_type = current.contact_type;
gr.assignment_group = gs.getProperty('2ndLevelSupport.group.sys_id'); //2nd level support group sys id fetching from sysProprties instead of hardcoding
gr.location = current.u_reference_1.location;

var customerservicecase= gr.insert();

GlideSysAttachment.copy('sn_customerservice_case', current.sys_id, 'incident', gr.sys_id); //copies attachment from case to newly created incident
current.incident= customerservicecase;
//current.u_incident= customerservicecase;////Considering there is a custom Incident field on customer service form that copies the sys_id of incident.
current.update();

action.setRedirectURL(gr);

}]]></client_script_v2>
